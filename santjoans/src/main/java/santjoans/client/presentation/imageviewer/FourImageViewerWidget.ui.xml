<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style>
		.image {
			border: 1px thin gray;
			margin: 4px;
		}
	</ui:style>
	
	<g:VerticalPanel>
		<g:cell>
			<g:HorizontalPanel>
				<g:cell horizontalAlignment='ALIGN_LEFT'>
					<g:Image ui:field="image1" styleName="{style.image}">
					</g:Image>
				</g:cell>
				<g:cell horizontalAlignment='ALIGN_RIGHT'>
					<g:Image ui:field="image2" styleName="{style.image}">
					</g:Image>
				</g:cell>
			</g:HorizontalPanel>
		</g:cell>
		<g:cell>
			<g:HorizontalPanel>
				<g:cell horizontalAlignment='ALIGN_LEFT'>
					<g:Image ui:field="image3" styleName="{style.image}">
					</g:Image>
				</g:cell>
				<g:cell horizontalAlignment='ALIGN_RIGHT'>
					<g:Image ui:field="image4" styleName="{style.image}">
					</g:Image>
				</g:cell>
			</g:HorizontalPanel>
		</g:cell>
	</g:VerticalPanel>
	
</ui:UiBinder> 